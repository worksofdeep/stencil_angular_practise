{
  "name": "angular-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build angular-form-library && ng build table-display-lib && ng build user-details-library",
    "watch": "ng build --watch --configuration development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.16",
    "@angular/cli": "^16.2.16",
    "@angular/compiler-cli": "^16.2.0",
    "@testing-library/angular": "^14.5.1",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.2.4",
    "ng-packagr": "^16.2.0",
    "typescript": "~5.1.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "./projects/my-app/src/setup.jest.ts"
    ],
    "testPathIgnorePatterns": [
      "./node_modules",
      "./dist/"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "./projects/my-app/tsconfig.spec.json",
        "stringifyContentPathRegex": "\\.html$"
      }
    }
  }
  
}
